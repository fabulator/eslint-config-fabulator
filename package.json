{
  "name": "eslint-config-fabulator",
  "version": "4.2.0",
  "description": "Shared ESLint config for fabulator projects",
  "keywords": [
    "eslint",
    "eslint-config",
    "eslintconfig"
  ],
  "homepage": "https://github.com/fabulator/eslint-config-fabulator",
  "bugs": {
    "url": "https://github.com/fabulator/eslint-config-fabulator/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fabulator/eslint-config-fabulator.git"
  },
  "license": "Apache-2.0",
  "author": "Michal Ozog√°n <michal@ozogan.eu>",
  "main": "index.js",
  "scripts": {
    "lint": "eslint ./ --ext .js --ext .json --config index.js --env node",
    "lint:fix": "npm run lint -- --fix",
    "release": "semantic-release",
    "test": "node node_modules/jest/bin/jest.js",
    "test-and-lint": "npm run test && npm run lint"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-array-func": "4.0.0",
    "eslint-plugin-canonical": "^5.0.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-typescript-sort-keys": "^3.2.0",
    "prettier": "^3.3.3"
  },
  "devDependencies": {
    "commitlint-config-fabulator": "^6.1.0",
    "enhanced-resolve": "^5.17.1",
    "jest": "^29.7.0",
    "semantic-release-config-fabulator": "^7.0.0",
    "typescript": "^5.5.4"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
