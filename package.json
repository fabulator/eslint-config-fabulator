{
  "name": "eslint-config-fabulator",
  "version": "3.1.0",
  "description": "Shared ESLint config for fabulator projects",
  "keywords": [
    "eslint",
    "eslint-config",
    "eslintconfig"
  ],
  "homepage": "https://github.com/fabulator/eslint-config-fabulator",
  "bugs": {
    "url": "https://github.com/fabulator/eslint-config-fabulator/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fabulator/eslint-config-fabulator.git"
  },
  "license": "Apache-2.0",
  "author": "Michal Ozog√°n <michal@ozogan.eu>",
  "main": "index.js",
  "scripts": {
    "lint": "eslint ./ --ext .js --ext .json --config index.js",
    "lint:fix": "npm run lint -- --fix",
    "release": "semantic-release",
    "test": "node node_modules/jest/bin/jest.js",
    "test-and-lint": "npm run test && npm run lint"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "^8.36.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-array-func": "^3.1.8",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-typescript-sort-keys": "^2.3.0",
    "prettier": "^3.1.1"
  },
  "devDependencies": {
    "commitlint-config-fabulator": "^5.0.0",
    "enhanced-resolve": "^5.12.0",
    "jest": "^29.5.0",
    "semantic-release-config-fabulator": "^6.0.0",
    "typescript": "^5.0.2"
  },
  "peerDependencies": {
    "typescript": "^4 || ^5"
  }
